#!/usr/bin/env node

/*
 * Auryn client
 */


/**
 * Module dependencies.
 */

var auryn = require('../package.json');
var program = require('commander');


program
  .version(auryn.version)
  .command('config', 'get / set auryn configuration variables')
  .command('setup', 'installs the environment for mobile flights')
  .parse(process.argv);
